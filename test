
set p1 { 1,1,1,1,1 };
print p1[1];
print p1[2];
print p1[3];
print p1[1+2];

print {1,1,1}+{2,2,2}[3];

set bp {1,0,0,0,0,1};
set v1 bp[0]; #<-----------
set v2 bp[1];
set v3 bp[ bp[1] ];
set v4 bp[ bp[ bp[1] ] ];

print v1;#<-----------
print v2;
print v3;
print v4;
set v1 v1 + v2 + v3 + v4;#<-----------
print v1;#<-----------

set px { 1,1,1 };
set py { 1,0,0,0,1 };
set pz px*py;

print pz;

print pz[0];
print pz[2];
print pz[8];
